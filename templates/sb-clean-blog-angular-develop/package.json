{
    "name": "sb-clean-blog-angular",
    "version": "1.2.0",
    "scripts": {
        "build": "npm run build:pug && npm run ng -- build --prod --build-optimizer=true --statsJson=true && npm run build:version",
        "build:debug": "npm run build:pug && npm run ng -- build --prod --source-map --build-optimizer=true --statsJson=true && npm run build:version",
        "build:demo": "npm run build && mv dist/sb-clean-blog-angular/assets/config-demo.json dist/sb-clean-blog-angular/assets/config.json",
        "build:pug": "node scripts/build-pug.js",
        "build:version": "node scripts/version.js",
        "bundle-report": "webpack-bundle-analyzer dist/sb-clean-blog-angular/stats-es2015.json",
        "docker:build": "node scripts/docker/docker-build.js",
        "docker:run": "node scripts/docker/docker-run.js",
        "drone:upload": "npm run ts-node -- scripts/drone/upload.ts",
        "e2e": "npm run ng -- e2e",
        "e2e:running": "npm run ng -- e2e --dev-server-target=",
        "generate:component": "ng generate @sbpro/ng:component",
        "generate:directive": "ng generate @sbpro/ng:directive",
        "generate:module": "ng generate @sbpro/ng:module",
        "generate:service": "ng generate @sbpro/ng:service",
        "link": "npm link @start-bootstrap/sb-clean-blog-shared-types",
        "lint:fix": "tslint -p . -c tslint.json --fix",
        "lint": "tslint -p . -c tslint.json",
        "ng": "cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node_modules/.bin/ng",
        "serve": "static-server dist/sb-clean-blog-angular",
        "serve:coverage": "static-server ./coverage/sb-clean-blog-angular 9753",
        "start": "node scripts/start.js",
        "test": "npm run ng -- test --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
        "test:dev": "node scripts/test-dev.js",
        "ts-node": "ts-node -r tsconfig-paths/register -r dotenv/config -P node_modules/@start-bootstrap/drone-utils/dist/tsconfig-drone.json",
        "version": "node scripts/version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StartBootstrap/sb-clean-blog-angular.git"
    },
    "bugs": {
        "url": "https://github.com/StartBootstrap/sb-clean-blog-angular/issues"
    },
    "homepage": "https://github.com/StartBootstrap/sb-clean-blog-angular#readme",
    "private": true,
    "dependencies": {
        "@angular/animations": "14.0.5",
        "@angular/common": "14.0.5",
        "@angular/compiler": "14.0.5",
        "@angular/core": "14.0.5",
        "@angular/forms": "14.0.5",
        "@angular/localize": "14.0.5",
        "@angular/platform-browser": "14.0.5",
        "@angular/platform-browser-dynamic": "14.0.5",
        "@angular/router": "14.0.5",
        "@auth0/angular-jwt": "5.0.2",
        "@fortawesome/angular-fontawesome": "0.11.1",
        "@fortawesome/fontawesome-svg-core": "6.1.1",
        "@fortawesome/free-brands-svg-icons": "6.1.1",
        "@fortawesome/free-regular-svg-icons": "6.1.1",
        "@fortawesome/free-solid-svg-icons": "6.1.1",
        "@ng-bootstrap/ng-bootstrap": "12.1.2",
        "@prettier/plugin-pug": "2.1.1",
        "bluebird": "^3.7.2",
        "change-case": "4.1.2",
        "chart.js": "3.8.0",
        "crypto-js": "4.1.1",
        "date-fns": "2.28.0",
        "marked": "4.0.18",
        "ngx-autosize": "1.8.7",
        "ngx-markdown": "14.0.1",
        "object-hash": "3.0.0",
        "prism": "^1.0.0",
        "rxjs": "7.5.6",
        "tslib": "2.4.0",
        "uuid": "8.3.2",
        "webpack-bundle-analyzer": "4.5.0",
        "zone.js": "0.11.6"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.0.5",
        "@angular/cli": "14.0.5",
        "@angular/compiler-cli": "14.0.5",
        "@angular/language-service": "14.0.5",
        "@google-cloud/storage": "6.2.2",
        "@inip/static-server": "1.0.1",
        "@sbpro/ng": "3.0.0",
        "@start-bootstrap/drone-utils": "0.1.6",
        "@start-bootstrap/sb-clean-blog-shared-types": "1.0.1",
        "@types/chart.js": "2.9.37",
        "@types/crypto-js": "4.1.1",
        "@types/jasmine": "4.0.3",
        "@types/jasminewd2": "2.0.10",
        "@types/marked": "4.0.3",
        "@types/node": "18.0.3",
        "@types/object-hash": "2.2.1",
        "@types/prismjs": "1.26.0",
        "@types/uuid": "8.3.4",
        "bootstrap": "5.1.3",
        "chokidar": "3.5.3",
        "codelyzer": "^0.0.28",
        "concurrently": "^7.2.2",
        "cross-env": "7.0.3",
        "jasmine-core": "4.2.0",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.0.0",
        "prettier": "2.7.1",
        "protractor": "7.0.0",
        "pug": "3.0.2",
        "pug-lint": "2.6.0",
        "shelljs": "0.8.5",
        "ts-node": "10.8.2",
        "tslint": "6.1.3",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "4.7.4"
    }
}
